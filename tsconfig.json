{
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~*": [
        "src/*"
      ],
      "~components/*": [
        "src/components/*"
      ],
      "~navigation/*": [
        "src/navigation/*"
      ],
      "~screens/*": [
        "src/screens/index"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
